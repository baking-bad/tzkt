{
  "Protocols": {
    "Diagnostics": false,
    "Validation": true
  },
  "TezosNode": {
    "Endpoint": "https://mainnet-tezos.giganode.io/",
    "Timeout": 60
  },
  "Quotes": {
    "Async": true,
    "Provider": {
      "Name": "TzktQuotes"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "host=db;port=5432;database=tzkt_db;username=tzkt;password=qwerty;"
  },
  "HealthChecks": {
    "Enabled": false,
    "Delay": 10,
    "Period": 10,
    "FilePath": "/tmp/sync.health"
  },
  "DipDupMetadata": {
    "Enabled": false,
    "DipDupUrl": "https://metadata.dipdup.net/v1/graphql",
    "Network": "mainnet",
    "BatchSize": 100,
    "PeriodSec": 60
  },                
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Tzkt.Sync.Services.DipDupMetadata": "Information"
    },
    "Console": {
      "FormatterName": "simple", // simple | systemd | json
      "FormatterOptions": {
        "UseUtcTimestamp": true
      }
    }
  }
}
