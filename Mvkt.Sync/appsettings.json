{
  "Protocols": {
    "Diagnostics": false,
    "Validation": true,
    "Fallback": null
  },
  "MavrykNode": {
    "Endpoint": "https://rpc.mavryk.network/basenet/",
    "Timeout": 60
  },
  "Quotes": {
    "Async": true,
    "Provider": {
      "Name": "MvktQuotes"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "host=db;port=5432;database=mvkt_db;username=mvkt-;password=qwerty;command timeout=600;"
  },
  "HealthChecks": {
    "Enabled": false,
    "Delay": 10,
    "Period": 10,
    "FilePath": "/tmp/sync.health"
  },
  "Domains": {
    "Enabled": false,
    "NameRegistry": "KT1GBZmSxmnKJXGMdMLbugPfLyUPmuLSMwKS",
    "PeriodSec": 30
  },
  "ContractMetadata": {
    "Enabled": false,
    "Period": 30,
    "DipDup": [
      {
        "Url": "https://metadata.dipdup.net/v1/graphql",
        "MetadataTable": "contract_metadata",
        "HeadStatusTable": "dipdup_head_status",
        "Network": "mainnet"
      }
    ]
  },
  "TokenMetadata": {
    "Enabled": false,
    "BatchSize": 100,
    "PeriodSec": 30,
    "OverriddenMetadata": [],
    "DipDup": [
      {
        "Url": "https://metadata.dipdup.net/v1/graphql",
        "MetadataTable": "token_metadata",
        "HeadStatusTable": "dipdup_head_status",
        "Network": "mainnet"
      },
      {
        "Url": "https://domains.dipdup.net/v1/graphql",
        "Network": "mainnet"
      },
      {
        "Url": "https://quartz.dipdup.net/v1/graphql",
        "Network": "mainnet"
      }
    ]
  },
  "MetricsOptions": {
    "Enabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "UseUtcTimestamp": true
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5001"
      }
    }
  },
  "AllowedHosts": "*"
}
